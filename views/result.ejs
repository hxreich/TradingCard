<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Top Tracks</title>
    <!-- <link rel="stylesheet" href="/css/styles.css"> Include your CSS file -->
</head>
<body>
    <h1>Top Tracks</h1>
    
    <% if (processedTracks.length === 0) { %>
        <p>No top tracks found for the given criteria.</p>
    <% } else { %>
        <ul>
            <% processedTracks.forEach((track) => { %>
                <li>
                    <strong>Track:</strong> <%= track.trackName %><br>
                    <strong>Artist:</strong> <%= track.artistName %>
                </li>
            <% }); %>
        </ul>
    <% } %>

    <a href="/">Back to Home</a>

    <!-- <script src="/js/scripts.js"></script>  Include your JavaScript file -->
</body>
</html>